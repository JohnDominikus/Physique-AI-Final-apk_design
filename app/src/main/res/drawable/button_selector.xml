<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Button pressed -->
    <item android:drawable="@color/green_pressed" android:state_pressed="true"/>
    <!-- Button focused -->
    <item android:drawable="@color/green_focused" android:state_focused="true"/>
    <!-- Default state -->
    <item android:drawable="@color/green" />
</selector>
